{
    "name": "dadapush/dadapush-php-client",
    "version": "1.0.0",
    "description": "DaDaPush: Real-time Notifications App\nSend real-time notifications through our API without coding and maintaining your own app for iOS or Android devices.",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api",
        "dadapush"
    ],
    "homepage": "https://www.dadapush.com/",
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "DaDaPush",
            "homepage": "https://www.dadapush.com/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "DaDaPushClient\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "DaDaPushClient\\" : "test/" }
    }
}
